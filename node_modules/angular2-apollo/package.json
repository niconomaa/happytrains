    "apollo-client": "^0.5.1",
    "ghooks": "^1.2.1",
    "graphql": "^0.7.0",
    "graphql-tag": "^0.1.9",
    "jest": "^17.0.0",
    "reflect-metadata": "^0.1.8",
    "remap-istanbul": "^0.7.0",
    "rimraf": "^2.5.2",
    "rxjs": "5.0.0-beta.12",
    "tslint": "^3.7.4",
    "typed-graphql": "^1.0.1",
    "typescript": "^2.0.6",
    "validate-commit-msg": "^2.6.1",
    "zone.js": "^0.6.23"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "testRegex": "\\.spec\\.js$",
    "testPathDirs": [
      "<rootDir>/build/tests"
    ],
    "browser": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/*.js",
      "!**/src/index.js",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "json"
    ]
  },
